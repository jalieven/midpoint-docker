accounts and entitlements into midPoint. The account-import task has a dependency        on the entitlement-import task which means it will run after the account-import task        has completed.    -->    <task oid="ef99c20d-215d-4c76-91ba-24380f342c59">        <name>Accounts import from PostgreSQL resource</name>        <extension xsi:type="c:ExtensionType">            <mext:kind>account</mext:kind>            <mext:intent>webidm</mext:intent>            <mext:workerThreads>10</mext:workerThreads>        </extension>        <!-- We need the entitlements to sync first before accounts -->        <dependent>1539330292502-0-2</dependent>        <taskIdentifier>1539330292502-0-1</taskIdentifier>        <ownerRef oid="00000000-0000-0000-0000-000000000002"/>        <executionStatus>suspended</executionStatus>        <dependent>1539330292502-0-2</dependent>        <handlerUri>http://midpoint.evolveum.com/xml/ns/public/model/synchronization/task/import/handler-3</handlerUri>        <objectRef oid="dafe0482-faef-47b7-ae08-66b150929bb7" type="c:ResourceType">        </objectRef>        <recurrence>single</recurrence><!--        <recurrence>recurring</recurrence>-->        <binding>tight</binding>        <schedule>            <interval>300</interval>            <misfireAction>reschedule</misfireAction>        </schedule>        <threadStopAction>suspend</threadStopAction>    </task>    <task oid="da99c20d-215d-4c76-91ba-24380f342c60">        <name>Entitlements import from PostgreSQL resource</name>        <extension xsi:type="c:ExtensionType">            <mext:kind>entitlement</mext:kind>            <mext:intent>privileges</mext:intent>            <mext:workerThreads>10</mext:workerThreads>        </extension>        <dependent>1580393809486-0-1</dependent>        <taskIdentifier>1539330292502-0-2</taskIdentifier>        <ownerRef oid="00000000-0000-0000-0000-000000000002"/>        <executionStatus>waiting</executionStatus>        <waitingReason>otherTasks</waitingReason>        <handlerUri>http://midpoint.evolveum.com/xml/ns/public/model/synchronization/task/import/handler-3</handlerUri>        <objectRef oid="dafe0482-faef-47b7-ae08-66b150929bb7" type="c:ResourceType">        </objectRef>        <recurrence>single</recurrence><!--        <recurrence>recurring</recurrence>-->        <binding>tight</binding>        <schedule>            <interval>300</interval>            <misfireAction>reschedule</misfireAction>        </schedule>        <threadStopAction>suspend</threadStopAction>    </task>    <task oid="37e6eee9-f92a-4e78-a634-7bdaf6eec8ec">        <name>Reconcile PostgreSQL accounts</name>        <extension xmlns:mext="http://midpoint.evolveum.com/xml/ns/public/model/extension-3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-3" xsi:type="c:ExtensionType">            <mext:kind>account</mext:kind>            <mext:intent>webidm</mext:intent>            <mext:objectclass>ri:AccountObjectClass</mext:objectclass>            <mext:workerThreads>10</mext:workerThreads>        </extension>        <taskIdentifier>1580393809486-0-1</taskIdentifier>        <ownerRef oid="00000000-0000-0000-0000-000000000002" relation="org:default" type="c:UserType">        </ownerRef>        <executionStatus>waiting</executionStatus>        <waitingReason>otherTasks</waitingReason>        <category>Reconciliation</category>        <handlerUri>http://midpoint.evolveum.com/xml/ns/public/model/synchronization/task/reconciliation/handler-3</handlerUri>        <objectRef oid="dafe0482-faef-47b7-ae08-66b150929bb7" relation="org:default" type="c:ResourceType">        </objectRef>        <recurrence>single</recurrence>        <binding>loose</binding>        <schedule>            <misfireAction>executeImmediately</misfireAction>        </schedule>        <threadStopAction>restart</threadStopAction>    </task></c:objects>