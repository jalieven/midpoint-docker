    <!--
        This task is needed to get the initial technical roles (groups) into LDAP.
        Since these are abstract roles that have a meta-role (with an inducement construction attached
        midPoint does not detect this at once and does not create these groups into the LDAP resource.
        Thus we nudge midPoint to recompute these roles.
    -->
    <task>
        <name>Recompute all application roles</name>
        <extension>
            <scext:executeScript xmlns:scext="http://midpoint.evolveum.com/xml/ns/public/model/scripting/extension-3">
                <s:pipeline xmlns:s="http://midpoint.evolveum.com/xml/ns/public/model/scripting-3">
                    <s:expression xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="s:SearchExpressionType">
                        <s:type>RoleType</s:type>
                        <s:query>
                            <q:filter>
                                <q:equal>
                                    <q:path>roleType</q:path>
                                    <q:value>application</q:value>
                                </q:equal>
                            </q:filter>
                        </s:query>
                    </s:expression>
                    <s:expression xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="s:ActionExpressionType">
                        <s:type>execute-script</s:type>
                        <s:parameter>
                            <s:name>script</s:name>
                            <c:value xsi:type="c:ScriptExpressionEvaluatorType">
                                <c:code>
                                    import com.evolveum.midpoint.xml.ns._public.common.common_3.RoleType
                                    log.info('Encountered Application Role {} to recompute', input.name)
                                    log.info('Role with oid {}', input.oid)
                                    log.info('Recomputing role {}', input.name)
                                    midpoint.recompute(RoleType.class, input.oid)
                                </c:code>
                            </c:value>
                        </s:parameter>
                    </s:expression>
              </s:pipeline>
              <options>
                  <continueOnAnyError>true</continueOnAnyError>
              </options>
            </scext:executeScript>
        </extension>
        <taskIdentifier>1579257412095-0-1</taskIdentifier>
        <ownerRef oid="00000000-0000-0000-0000-000000000002"/>
        <executionStatus>runnable</executionStatus>
        <category>BulkActions</category>
        <handlerUri>http://midpoint.evolveum.com/xml/ns/public/model/scripting/handler-3</handlerUri>
        <recurrence>single</recurrence>
    </task>

    <task>
        <name>Recompute the meta-role</name>
        <extension>
            <scext:executeScript xmlns:scext="http://midpoint.evolveum.com/xml/ns/public/model/scripting/extension-3">
                <s:pipeline xmlns:s="http://midpoint.evolveum.com/xml/ns/public/model/scripting-3">
                    <s:expression xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="s:SearchExpressionType">
                        <s:type>RoleType</s:type>
                        <s:query>
                            <q:filter>
                                <q:equal>
                                    <q:path>roleType</q:path>
                                    <q:value>meta</q:value>
                                </q:equal>
                            </q:filter>
                        </s:query>
                    </s:expression>
                    <s:expression xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="s:ActionExpressionType">
                        <s:type>execute-script</s:type>
                        <s:parameter>
                            <s:name>script</s:name>
                            <c:value xsi:type="c:ScriptExpressionEvaluatorType">
                                <c:code>
                                    import com.evolveum.midpoint.xml.ns._public.common.common_3.RoleType
                                    log.info('Encountered Meta Role {} to recompute', input.name)
                                    log.info('Role with oid {}', input.oid)
                                    log.info('Recomputing role {}', input.name)
                                    midpoint.recompute(RoleType.class, input.oid)
                                </c:code>
                            </c:value>
                        </s:parameter>
                    </s:expression>
                </s:pipeline>
                <options>
                    <continueOnAnyError>true</continueOnAnyError>
                </options>
            </scext:executeScript>
        </extension>
        <taskIdentifier>3974257412095-0-1</taskIdentifier>
        <ownerRef oid="00000000-0000-0000-0000-000000000002"/>
        <executionStatus>runnable</executionStatus>
        <category>BulkActions</category>
        <handlerUri>http://midpoint.evolveum.com/xml/ns/public/model/scripting/handler-3</handlerUri>
        <recurrence>single</recurrence>
    </task>

    <task>
        <name>Reconcile LDAP groups</name>
        <extension xmlns:mext="http://midpoint.evolveum.com/xml/ns/public/model/extension-3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-3" xsi:type="c:ExtensionType">
            <mext:kind>entitlement</mext:kind>
            <mext:intent>group</mext:intent>
            <mext:objectclass>ri:groupOfUniqueNames</mext:objectclass>
        </extension>
        <taskIdentifier>1579604436378-0-1</taskIdentifier>
        <ownerRef oid="00000000-0000-0000-0000-000000000002" />
